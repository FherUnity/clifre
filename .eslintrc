{
  "extends": ["eslint:recommended", "airbnb"],
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "import/no-unresolved": "off",
    "require-jsdoc": "off",
    "switch-colon-spacing": "off",
    "comma-dangle": [
      "error",
      {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "imports": "always-multiline",
        "exports": "always-multiline",
        "functions": "ignore"
      }
    ]
  }
}
